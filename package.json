    "@types/es6-promise": "0.0.32",
    "@types/load-json-file": "^2.0.5",
    "@types/mocha": "^2.2.33",
    "@types/tv4": "^1.2.28",
    "@types/ws": "0.0.37",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chai-json-schema": "^1.4.0",
    "load-json-file": "^2.0.0",
